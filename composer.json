{
    "secure-http" : false,
    "require": {
        "php": ">=5.5.9",
        "vlucas/phpdotenv": "~1.0",
        "illuminate/database":"5.1.*",
        "monolog/monolog": "1.*",
        "Pvol/FlowMatrix": "@stable",
        "twbs/bootstrap" : "3.*"
    },
    "require-dev": {
        "filp/whoops": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Xisnear\\Frame\\": "app/Library/xisnear/frame/",
            "Xisnear\\Flow\\": "app/Library/xisnear/flow/",
            "Xisnear\\Rule\\": "app/Library/xisnear/rule/",
            "Xisnear\\Openapi\\": "app/Library/xisnear/openapi/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {"type": "composer", "url": "http://packagist.phpcomposer.com"},
        {"packagist": false}
    ]
}
