{
    "secure-http" : false,
    "require": {
        "php": ">=5.5.9",
        "vlucas/phpdotenv": "~1.0",
        "illuminate/database":"5.1.*",
        "monolog/monolog": "1.*",
        "predis/predis": "~1.1"
    },
    "require-dev": {
        "filp/whoops": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "app/Library/Core/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {"type": "composer", "url": "http://packagist.phpcomposer.com"},
        {"packagist": false}
    ]
}
